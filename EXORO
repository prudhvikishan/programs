#include <iostream>
#include<string.h>
using namespace std;
 
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
    	long int n;
    	long int e,o,count;
    	count=0;
    	e=0;
    	o=0;
    	 long int arr[100000];
    	cin>>n;
    	for(int i=0;i<n;i++)
    	{
    		cin>>arr[i];
    	}
    	for(int j=0;j<n;j++)
    	{
    		if(arr[j]%2==0)
    		{
    		e++;
    		}
    		else
    		{
    			o++;
    		}
    	}
    	count = e*o;
    	cout<<count<<"\n";
    }
   return 0;
}
